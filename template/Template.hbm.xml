 <property name="RSI" type="double" column="RSI"/>
 <property name="ADL" type="integer" column="ADL"/>
 <property name="SMA20Day" type="double" column="SMA20Day"/>
 <property name="SMA50Day" type="double" column="SMA50Day"/>
 <property name="SMA100Day" type="double" column="SMA100Day"/>
 <property name="SMA200Day" type="double" column="SMA200Day"/>
 <property name="CommodityChannelIndex" type="double" column="CommodityChannelIndex"/>
