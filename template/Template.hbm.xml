 <property name="EMA3Day" type="double" column="EMA3Day"/>
 <property name="EMA15Day" type="double" column="EMA15Day"/>
 <property name="EMA5Day" type="double" column="EMA5Day"/>
 <property name="EMA20Day" type="double" column="EMA20Day"/>
 <property name="EMA12Day" type="double" column="EMA12Day"/>
 <property name="EMA9Day" type="double" column="EMA9Day"/>
 <property name="EMA26Day" type="double" column="EMA26Day"/>
 <property name="EMATrend" type="double" column="EMATrend"/>
 <property name="EMARecommendation" type="double" column="EMARecommendation"/>
 </class>
</hibernate-mapping>

